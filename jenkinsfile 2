pipeline{
agent any
  tools{
     maven 'mymaven'
  }

stages{
stage('clone Repo')
  {
    steps{
       git 'https://github.com/Nag2553/DevOpsClassCodes.git'    
    }
  }
stage('Build Code')
  {
    steps{
       sh 'mvn package'   
    }
  }

stage('Deploy Code')
  {
    steps{
      deploy adapters: [tomcat9(credentialsId: 'tomcatcred', path: '', url: 'http://3.112.209.16:9090/')], contextPath: null, war: '**/*.war'
    }
  }
}

  
}
